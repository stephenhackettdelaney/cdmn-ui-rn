{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"border-4": {
		"style": {
			"borderTopWidth": 4,
			"borderRightWidth": 4,
			"borderBottomWidth": 4,
			"borderLeftWidth": 4
		}
	},
	"border-red-400": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(248 113 113 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(248 113 113 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(248 113 113 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(248 113 113 / var(--tw-border-opacity))"
		}
	},
	"text-red-400": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(248 113 113 / var(--tw-text-opacity))"
		}
	}
}